	Arraylist mytest;
	init_arraylist(&mytest, sizeof(char));
	char char1='k';
	append(&mytest,&char1);
	char char2='a';
	append(&mytest,&char2);
	char char3='n';
	append(&mytest,&char3);
	char char4='g';
	append(&mytest,&char4);
	char char5='a';
	append(&mytest,&char5);
	char char6='r';
	append(&mytest,&char6);
	char char7='o';
	append(&mytest,&char7);
	char char8='o';
	append(&mytest,&char8);
	char insertChar1='w';
	insert(&mytest,&insertChar1,3);
	printCharArraylist(&mytest);
	char insertChar2='v';
	insert(&mytest,&insertChar2,5);
	printCharArraylist(&mytest);
	
	delete(&mytest, 5);
	printf("after delete the 5th position\n");
	printCharArraylist(&mytest);
	delete(&mytest,3);
	printf("after delete the 3th position\n");
	printCharArraylist(&mytest);
	
	
	int int1=2;
	int int2=0;
	int int3=1;
	int int4=7;
	int int5=6;
	int int6=2;
	int int7=1;
	Arraylist myIntTest;
	init_arraylist(&myIntTest,sizeof(int));
	append(&myIntTest,&int1);
	append(&myIntTest,&int2);
	append(&myIntTest,&int3);
	append(&myIntTest,&int4);
	append(&myIntTest,&int5);
	append(&myIntTest,&int6);
	append(&myIntTest,&int7);
	printIntArraylist(&myIntTest);
	int insertInt1=10;
	insert(&myIntTest,&insertInt1,3);
	int insertInt2=19;
	insert(&myIntTest,&insertInt2,6);
	printf("After insert the two interger\n");
	printIntArraylist(&myIntTest);
	
	delete(&myIntTest,6);
	printf("After delete the first position integer\n");
	printIntArraylist(&myIntTest);
	delete(&myIntTest,3);
	printf("After delete the second position integer\n");
	printIntArraylist(&myIntTest);
